<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baosight.dao.ShopCarVOMapper" >
    <resultMap id="BaseResultMap" type="com.baosight.VO.ShoppingCarVO" >
        <result property="name" column="name"/>
        <result property="price" column="price"/>
        <result property="num" column="num"/>
        <result property="status" column="status"/>
        <result property="creatTime" column="creatTime"/>
    </resultMap>

    <select id="shopCarSelete" resultMap="BaseResultMap">
    select s.foodId,f.name,f.price,s.creatTime,s.num,s.status
    from food f,shoppingcart s
    where s.foodId = f.id
  </select>
</mapper>